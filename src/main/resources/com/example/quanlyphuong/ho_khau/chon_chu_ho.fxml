                <TableColumn fx:id="hoTen" prefWidth="75.0" text="Họ tên" />
                <TableColumn fx:id="gioiTinh" prefWidth="75.0" text="Giới tính" />
                <TableColumn fx:id="ngaySinh" prefWidth="113.0" text="Ngày sinh" />
                <TableColumn fx:id="soCMT" prefWidth="137.0" text="Số CMT" />
                <TableColumn fx:id="diaChiHienNay" prefWidth="338.0" text="Địa chỉ hiện nay" />
            </columns>
        </TableView>
        <Button fx:id="btnHuy" layoutX="641.0" layoutY="410.0" mnemonicParsing="false" onAction="#huy" text="Hủy" />
    </children>
</AnchorPane>
