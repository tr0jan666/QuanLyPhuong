<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="1231.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.quanlyphuong.controllers.ho_khau.SuaController">
   <children>
      <TableView fx:id="dataTable" prefHeight="400.0" prefWidth="566.0">
        <columns>
            <TableColumn fx:id="IDColumn" prefWidth="44.0" text="ID" />
          <TableColumn fx:id="hoTen" prefWidth="170.0" text="Họ tên" />
          <TableColumn fx:id="gioiTinh" prefWidth="74.0" text="Giới tính" />
            <TableColumn fx:id="ngaySinh" prefWidth="116.0" text="Ngày sinh" />
            <TableColumn fx:id="soCMT" prefWidth="161.0" text="Số CMT" />
        </columns>
      </TableView>
      <Button layoutX="612.0" layoutY="144.0" mnemonicParsing="false" onAction="#add" text="&gt;&gt;" />
      <Button layoutX="612.0" layoutY="189.0" mnemonicParsing="false" onAction="#remove" text="&lt;&lt;" />
      <Button layoutX="587.0" layoutY="231.0" mnemonicParsing="false" onAction="#save" text="Lưu thay đổi" />
      <TableView fx:id="addedDataTable" layoutX="715.0" prefHeight="400.0" prefWidth="516.0">
        <columns>
          <TableColumn fx:id="hoTenAdded" prefWidth="197.0" text="Họ tên" />
          <TableColumn fx:id="ngaySinhAdded" prefWidth="140.0" text="Ngày sinh" />
            <TableColumn fx:id="quanHeVoiChuHo" prefWidth="178.0" text="Quan hệ với chủ hộ" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
